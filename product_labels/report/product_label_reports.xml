    <record id="action_report_product_label_60x40" model="ir.actions.report">
        <field name="name">Product Labels 60x40 mm</field>
        <field name="model">print.product.label.line</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="paperformat_label_60x40" />
        <field name="report_name">product_labels.report_product_label_60x40_template</field>
        <field name="report_file">product_labels.report_product_label_60x40_template</field>
        <field name="print_report_name">'Product Labels 60x40mm'</field>
    </record>

    <record id="action_report_product_label_A4_57x35" model="ir.actions.report">
        <field name="name">Product Labels 57x35 mm (A4, 21 pcs)</field>
        <field name="model">print.product.label.line</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="paperformat_label_a4_blank" />
        <field name="report_name">product_labels.report_product_label_57x35_template</field>
        <field name="report_file">product_labels.report_product_label_57x35_template</field>
