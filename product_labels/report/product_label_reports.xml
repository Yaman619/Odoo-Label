        <field name="name">Labels from your own template</field>
        <field name="model">print.product.label.line</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="product_labels.paperformat_label_a4_blank" />
        <field name="report_name">product_labels.report_product_label_from_template</field>
        <field name="report_file">product_labels.report_product_label_from_template</field>
